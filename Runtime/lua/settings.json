{
  "serverExecutableName": "engine/PhrostIPC",
  "clientInterpreterName": "runtime/lua",
  "clientScriptName": "game/main.lua",
  "clientInterpreterParams": [
    "-dopcache.enable=1",
    "-dopcache.enable_cli=1",
    "-dopcache.jit_buffer_size=100M",
    "-dopcache.jit=tracing"
  ],
  "watchDirectoryName": "game",
  "watchExclusions": ["/**/node_modules", "/**/vendor"],
  "clientLogging": true,
  "ipcMode": "sockets",
  "ipcPort": 8888
}
